<h2>Search Results</h2>

@if (ViewBag.SearchResults.Count == 0)
{
  @: no results found!
}
<ul>
  @foreach (var item in ViewBag.SearchResults)
  {
    <li>
      <iframe title="deezer-widget" src="https://widget.deezer.com/widget/dark/@item" width="100%" height="140"
      frameborder="0" allowtransparency="true" allow="encrypted-media; clipboard-write"></iframe>
    </li>
  }

</ul>